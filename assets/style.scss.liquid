////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 1. CSS RESET ////////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

html, body, div, span, applet, object, iframe, h1, h2, h3, h4, h5, h6, p, blockquote, pre, a, abbr, acronym, address, big, cite, code, 
del, dfn, em, font, img, ins, kbd, q, s, samp, small, strike, strong, sub, sup, tt, var, b, u, i, center, dl, dt, dd, ol, ul, li, 
fieldset, form, label, legend, table, caption, tbody, tfoot, thead, tr, th, td, input, textarea, button
{margin: 0;padding: 0;border: 0;outline: none !important;vertical-align: top;background: transparent;font-weight: normal;line-height: 1.5em;}

ol, ul {list-style:none;}
a, span {vertical-align:baseline;}
b, strong {font-weight: bold;}
i, dfn {font-style: italic;}

img {max-width: 100%;}

body {font: normal {{ settings.base_font_size }} {{ settings.base_font | replace:'google_fonts:' }};color: {{ settings.base_font_color }}; position: inherit !important;}




////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 2. VARIABLES & MIXINS ///////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// 2.1 VARIABLES ///////////////////////////////////////////////////////////////////////////////////////////////////
$base_font:          {{ settings.base_font | replace:'google_fonts:' }};
$base_font_size:     {{ settings.base_font_size }};
$base_font_color:    {{ settings.base_font_color }};


$heading_font:       {{ settings.heading_font | replace:'google_fonts:' }};
$heading_font_size:  {{ settings.heading_font_size }};
$heading_font_color: {{ settings.heading_font_color }};


$bg_color:           {{ settings.body_bg_color }};
$border_color:       {{ settings.border_color }};


{% unless settings.color_scheme_switch %}
	{% capture color_1 %}scheme_{{ settings.color_scheme }}_color_1{% endcapture %}
	{% capture color_2 %}scheme_{{ settings.color_scheme }}_color_2{% endcapture %}

	$color_1:            {{ settings[color_1] }};
	$color_2:            {{ settings[color_2] }};
{% endunless %}

	$color_3:            {{ settings.color_3 }};
	$color_4:            {{ settings.color_4 }};
	$color_5:            {{ settings.color_5 }};




// 2.2 MIXINS //////////////////////////////////////////////////////////////////////////////////////////////////////
@mixin border_radius($r){
-webkit-border-radius: $r;
   -moz-border-radius: $r;
		border-radius: $r;
}


@mixin transition($t, $a:0, $b:0, $c:0) {
// -webkit-transform: translate3d(0, 0, 0);
//     -ms-transform: translate3d(0, 0, 0);
//      -o-transform: translate3d(0, 0, 0);
// 		transform: translate3d(0, 0, 0);

    @if $a == 0 and $b == 0 and $c == 0 {
    -webkit-transition: all $t ease-in-out;
       -moz-transition: all $t ease-in-out;
            transition: all $t ease-in-out;
    }

    @if $a != 0 and $b == 0 and $c == 0 {
    -webkit-transition: $a $t ease-in-out;
       -moz-transition: $a $t ease-in-out;
            transition: $a $t ease-in-out;
    }

    @if $a != 0 and $b != 0 and $c == 0 {
    -webkit-transition: $a $t ease-in-out, $b $t ease-in-out;
       -moz-transition: $a $t ease-in-out, $b $t ease-in-out;
            transition: $a $t ease-in-out, $b $t ease-in-out;
    }

    @if $a != 0 and $b != 0 and $c != 0 {
    -webkit-transition: $a $t ease-in-out, $b $t ease-in-out, $c $t ease-in-out;
       -moz-transition: $a $t ease-in-out, $b $t ease-in-out, $c $t ease-in-out;
            transition: $a $t ease-in-out, $b $t ease-in-out, $c $t ease-in-out;
    }
}


@mixin box_shadow($x...) {
-webkit-box-shadow: $x;
   -moz-box-shadow: $x;
		box-shadow: $x;
}


@mixin transform($y...) {
-webkit-transform: $y;
		transform: $y;
}


@mixin transform_origin($y...) {
-webkit-transform-origin: $y;
		transform-origin: $y;
}


@mixin background_size($s...){
-webkit-background-size: $s;
   -moz-background-size: $s;
		background-size: $s;
}




{% capture styles %}
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 3. GLOBAL THEME STYLES //////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// 3.1 MISCELLANEOUS ///////////////////////////////////////////////////////////////////////////////////////////////

// hiding placeholder text on focus in webkit, thanks to http://www.kolodvor.net/2012/03/23/webkit-placeholder-attribute-behavior
input:focus::-webkit-input-placeholder,
textarea:focus::-webkit-input-placeholder {color: transparent;}


// clearfix - nicolasgallagher.com/micro-clearfix-hack/
.clearfix {zoom: 1;
	&:before,
	&:after {content: "";display: table;}

	&:after {clear: both;}
}


// utility classes: hidden, float left, float right, etc.
.hidden,
.visuallyhidden {display: none;}
 
.left {float: left;}
.right {float: right;}


// jquery.formstyler styles for selects
.jq-selectbox {display: block;z-index: 1000 !important;cursor: pointer;}
.jq-selectbox.opened {}
.jq-selectbox.focused {}

.jq-selectbox__select {}

.jq-selectbox__select-text {width: 100%;display: block;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;}
.jq-selectbox__trigger i {position: absolute;top: 0;right: 0;
@include transition(.3s);
}
.jq-selectbox__dropdown {width: 100%;
@include transition(.3s);
}

.jq-selectbox.opened .jq-selectbox__trigger i {
@include transform(rotate(180deg));
}


// other
#at15s {z-index: 999 !important;}

#habla_window_div {-webkit-backface-visibility: hidden;-webkit-transform: translateZ(0);
	textarea {min-height: 1px;}
}


// global loader
.global_loader {width: 44px;height: 44px;margin: 0 0 0 -22px;position: absolute;left: 50%;top: 0;z-index: 1001;
background: url('data:image/png;base64,') 50% 50% no-repeat;
@include transition(.3s, top, opacity);
}
.global_loader:after {content: '';width: 100%;height: 100%;display: block;background: url('fancybox_loading.gif') 50% 50% no-repeat;}

.loader_on .global_loader {top: 20px;
opacity: 1;
}
.loader_off .global_loader {top: 0;
opacity: 0;
}


// rd-parallax
.rd-parallax-inner {min-height: 100%;overflow: hidden;position: relative;}
.rd-parallax-layer[data-type="media"] {height: 100%;position: absolute;top: 0;left: 0;right: 0;bottom: 0;
-webkit-backface-visibility: hidden;
}
.rd-parallax-layer[data-type="media"] iframe {width: 100%;height: 100%;}
.rd-parallax-layer[data-url] {background-position: center 25%;background-repeat: no-repeat;
-webkit-filter: blur(0) !important;
		filter: blur(0) !important;

// -webkit-background-size: cover;
//         background-size: cover;
}
.rd-parallax-layer[data-type="html"] {position: relative;}




// 3.2 PAGE PRELOADER //////////////////////////////////////////////////////////////////////////////////////////////
#page_preloader {width: 100%;height: 100%;background: $bg_color;position: fixed;left: 0;top: 0;z-index: 1000;
opacity: 1;
@include transition(.6s, opacity);

	.global_loader {margin: -22px 0 0 -22px;top: 50%;}

	&.off {
	opacity: 0;
	}
}




// 3.3 COOKIES POLICY MESSAGE //////////////////////////////////////////////////////////////////////////////////////
#cookies_container {min-height: 60px;background: #191919;text-align: center;
	display: none;

	p {display: inline-block;margin: 0;font-size: 17px;line-height: 60px;color: #888;
		b {vertical-align: baseline;color: #fff;}
	}

	.cookie_btn {height: 40px;display: inline-block;margin: 10px 0 10px 6px;padding: 0 20px;border-width: 1px;border-style: solid;font-weight: bold;font-size: 17px;line-height: 38px;color: #fff;
		@include border_radius(4px);
		@include transition(.3s);
	}

	#cookie_ok {margin: 10px 0 10px 20px;background: #18a439;border-color: #18a439;
		&:hover {background: #585858;border-color: #585858;}
	}
	#cookie_no {border-color: #585858;
		&:hover {background: #585858;border-color: #585858;}
	}
}




// 3.4 GOOGLE MAPS /////////////////////////////////////////////////////////////////////////////////////////////////
#google_map,
#google_map iframe {width: 100% !important;height: 300px !important;background: #e7e7e6;}

#google_map img {max-width: none !important;}




// 3.5 FANCYBOX COMMON STYLES //////////////////////////////////////////////////////////////////////////////////////
.fancybox-skin {padding: 15px;
@include border_radius(0);
}

.fancybox-close {width: auto;height: auto;display: block;vertical-align: top;color: $color_1;right: 30px;top: 30px;
	&:hover {color: $color_2;}
	&:before {content: '\e01e';display: block;font: 24px/1em 'fl-bigmug-line';}
}

.fancybox-nav {
	span {width: 40px;height: 40px;display: block;background: rgba(255,255,255,.5);color: $color_1;text-align: center;
	@include border_radius(50%);
	@include transition(.3s, background, color);

		&:before {display: block;font: 23px/40px 'arrows';}
	}

	&:hover > span {background: $color_3;}
}

.fancybox-prev span {margin: -20px 0 0 10px;}
.fancybox-prev span:before {content: '\e00f';}

.fancybox-next span {margin: -20px 10px 0 0;}
.fancybox-next span:before {content: '\e00d';}




// 3.6 ADD TO CART MESSAGE /////////////////////////////////////////////////////////////////////////////////////////
#cart_added__wrap {
	.fancybox-skin {padding: 60px !important;}
}

#cart_added {width: 1000px;
	.cart_added__row {}
}

.cart_added__1 {width: 300px;height: 100%;float: left;
	img {}
}

.cart_added__2 {margin: 0 0 0 360px;
	h4 {margin: 0;font-size: 20px;letter-spacing: 3px;}

	.product_name {display: block;padding: 40px 0 0 0;letter-spacing: 7px;}
	#cart_added__quantity {margin: 0;padding: 35px 0 0 0;font-size: 16px;}
	.btn {margin: 40px 30px 0 0;padding: 12px 20px;font-size: 16px;}
}




// 3.7 PRODUCT QUICK VIEW //////////////////////////////////////////////////////////////////////////////////////////
#quick_view__wrap {
	.fancybox-skin {padding: 60px !important;}

	.fancybox-inner {overflow: hidden !important;}
}

#product_quick_view {width: 1350px;overflow: hidden;}

#quick_view__left {width: 480px;float: left;padding: 0 50px;position: relative;
	#img_gallery {width: 380px;}

	a {cursor: default;}

	span {width: 70px;height: 70px;display: block;font-size: 10px;line-height: 70px;text-align: center;text-transform: uppercase;position: absolute;top: 10px;z-index: 100;cursor: default;
	@include border_radius(50%);
	}
	#product_new {background: rgba($color_1, .1);color: $color_1;right: 60px;}
	#product_sale {background: $color_1;color: $color_3;left: 60px;}

	.swiper_btn {height: 40px;margin: -20px 0 0 0;color: $color_2;text-align: center;position: absolute;top: 50%;
	text-shadow: 1px 1px 0 rgba(255, 255, 255, .7);

		&:hover {color: $color_1;}
		&:before {display: block;font: 30px/40px "Arrows";cursor: pointer;}
	}

	#img_gallery__prev {left: 0;}
	#img_gallery__prev:before {content: '\e00f';}

	#img_gallery__next {right: 0;}
	#img_gallery__next:before {content: '\e00d';}
}

#quick_view__right {max-height: 100% !important;overflow: auto;margin: 0 0 0 540px;
	.quick_view__tabs {
		a {margin: 0 65px 0 0;font-weight: 300;font-size: 20px;color: $color_2;text-transform: uppercase;letter-spacing: 5px;
			&.active {color: $color_1;}
		}
	}

	.tabs_content {padding: 40px 0 0 0;}

	#tab_1,
	#tab_2 {overflow: auto;
	opacity: 0;
	@include transition(.15s, opacity);
		&.active {opacity: 1;}
		&.hidden {display: none;}
	}

	.product_name {margin: 0;font-size: 30px;line-height: 2em;letter-spacing: 7px;}

	#quick_view__description {padding: 0 15px 0 0;color: $color_2;
		iframe {width: 100% !important;height: 100% !important;}
		a {color: $color_1;text-decoration: underline;}
		a:hover {text-decoration: none;}
	}
}

#quick_view__price {padding: 20px 0;
	.money {margin: 0 15px 0 0;font-weight: bold;font-size: 40px;}
	.money_sale {font-style: italic;font-weight: 300;font-size: 30px;}
}

#quick_view__variants {width: 380px;margin: 0;
	.selector-wrapper {margin: 30px 0 0 0;
		label {display: none !important;}
		select {width: 100%;height: 50px;display: block;border: 1px solid $color_1;font-size: 20px;color: $color_1;text-align: center;text-transform: uppercase;}
	}
}

#quick_view__form {width: 380px;padding: 30px 0 0 0;
	.quantity_box {float: left;margin: 0;}
	.btn_wrapper {margin: 0 0 0 62px;}
	#quick_view__add {width: 100%;height: 60px;padding: 0 20px;background: $color_1 !important;color: $color_3 !important;font-size: 18px;
	@include transition(.15s, background);
		&:before {
		@include transition(.15s, border);
		}
		&:hover {background: $color_2 !important;border: none !important;
			&:before {border-color: $color_2 !important;}
		}

	}
}




////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 4. TYPOGRAPHY ///////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

body {overflow: visible;font: normal $base_font_size $base_font;color: $base_font_color;}




// 4.1 HEADINGS ////////////////////////////////////////////////////////////////////////////////////////////////////
h1,
h2,
h3,
h4,
h5,
h6 {margin: 15px 0;font: 300 $heading_font_size $heading_font;color: $heading_font_color;text-transform: uppercase;}

h2,
h3,
h4 {font-size: 16px;}

h5,
h6 {font-size: 14px;}

.page_heading {margin: 0;font-weight: 300;font-size: $heading_font_size;letter-spacing: 6px;
	a {font-weight: inherit;}
}

h1.page_heading {margin: 120px 0 0 0;line-height: 60px;}

.heading_counter {height: 60px;display: block;margin: 0 0 0 15px;font-weight: 300;font-size: 60px;line-height: 56px;position: relative;
	&:before {content: '';border-top: 1px solid adjust-color($color_1, $red: 48, $green: 48, $blue: 48);position: absolute;top: 52%;}
}
.heading_counter__right {float: right;
	&:before {width: 765px;right: -139px;}
}




// 4.2 PARAGRAPHS & LISTS //////////////////////////////////////////////////////////////////////////////////////////
p {margin: 15px 0;padding: 0;}

.column_center {
	ul {list-style-type: square;list-style-position: inside;}
	ol {list-style-type: decimal;list-style-position: inside;}
}




// 4.3 LINKS ///////////////////////////////////////////////////////////////////////////////////////////////////////
a {color: {{ settings.link_color }};text-decoration: none;
	@include transition(.3s, background, border-color, color);

	&:hover,
	&:active {color: {{ settings.link_color_hover }};}
}




// 4.4 PRODUCT ATTRIBUTES (name, description, price) ///////////////////////////////////////////////////////////////
.product_name {font-weight: 300;font-size: {{ settings.product_name_size }};color: {{ settings.product_name_color }};text-transform: uppercase;
	a {font-weight: 300;color: {{ settings.product_name_color }};
		@include transition(.3s);
		&:hover {color: {{ settings.product_name_color_hover }};}
	}
}


.product_desc {font-size: {{ settings.product_desc_size }};color: $base_font_color;}


.money {color: {{ settings.product_price_color }};}

.money_sale,
.compare-at-price {text-decoration: line-through;}




// 4.5 RTE /////////////////////////////////////////////////////////////////////////////////////////////////////////
.rte {margin-top: 15px;margin-bottom: 15px;color: inherit;
	& > * {max-width: 100% !important;margin: 40px 0 0 0;font-weight: 300;}

	ol li,
	ul li {font-weight: 300;}

	a {text-decoration: underline;
		&:hover {text-decoration: none;}
	}

	h1,
	h2,
	h3,
	h4,
	h5,
	h6 {margin: 40px 0 0 0;font-weight: 300;letter-spacing: 3px;}

	h1 {font-size: 40px;}
	h2 {font-size: 35px;}
	h3 {font-size: 30px;}
	h4 {font-size: 25px;}
	h5 {font-size: 20px;}
	h6 {font-size: 16px;}

	ul {list-style-type: disc;list-style-position: inside;}
	ol {list-style-type: decimal;list-style-position: inside;}

	blockquote {padding: 5px 35px;font-weight: normal;
		&:before {content: '\f10d';font: 16px 'FontAwesome';}
		&:after {content: '\f10e';float: right;font: 12px 'FontAwesome';}
		& * {margin: 10px 0 !important;font-size: 15px;}
	}
}


.youtube_wrap {padding: 56.25% 0 0 0;position: relative;
	iframe {width: 100% !important;height: 100% !important;position: absolute;left: 0;top: 0;}
}


.page_content {
	.rte {
		table {width: 100%;max-width: 100%;
			td {padding: 0 15px;
				img {width: auto;max-width: 100% !important;}
			}

			h2,
			h3,
			h4,
			h5,
			h6 {margin: 15px 0;padding: 0;border: none;font-weight: normal;color: $color_2;text-transform: uppercase;}

			h4 {font-size: 18px;}
		}
	}
}




////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 5. FORMS & COMPONENTS ///////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

// 5.1 FORMS ///////////////////////////////////////////////////////////////////////////////////////////////////////
label {font-size: $base_font_size;color: $color_1;}

select,
select.form-control,
textarea,
textarea.form-control,
input.form-control,
input[type="text"],
input[type="email"],
input[type="number"],
input[type="password"],
input[type="telephone"] {height: 40px;padding: 0;border: 1px solid $border_color;border-width: 0 0 1px 0;font-weight: 300;font-size: 14px;color: $base_font_color;
-webkit-box-sizing: border-box;
		box-sizing: border-box;

@include border_radius(0);
@include box_shadow(none);
}

select,
select.form-control {padding: 10px 0;}

textarea,
textarea.form-control {min-width: 100%;max-width: 100%;min-height: 100px;}

select:focus,
select.form-control:focus,
textarea:focus,
textarea.form-control:focus,
input[type="text"]:focus,
input[type="email"]:focus,
input[type="number"]:focus,
input[type="password"]:focus,
input[type="telephone"]:focus {border-color: #888;
@include box_shadow( none !important );
}


input[type="radio"],
input[type="checkbox"],
.form-group {vertical-align: top !important;margin-top: 15px;margin-bottom: 0;}


.form-horizontal {
	label {padding-top: 0 !important;text-align: right;line-height: 34px;}
	select {width: 100%;}
}

.action_bottom span.note {margin-left: 5px;}
.action_bottom span.note a {margin-left: 5px;}


.form_big {
	input,
	textarea {margin: 40px 0 0 0;padding: 22px 0;font-size: 18px;border-width: 0 0 1px 0;
	@include box_shadow( none !important );
	}

	input {height: auto;}
	textarea {height: 265px;}

	.btn {margin: 160px 0 0 0;
		&:before {right: 39%;}
	}
}




// 5.2 ALERTS & NOTIFICATIONS //////////////////////////////////////////////////////////////////////////////////////
.alert {margin: 15px 0;padding: 15px 20px;font-weight: normal;
@include border_radius(0);

	p {margin: 0;}

	a {font-weight: bold;text-decoration: underline;
		&:hover {text-decoration: none;}
	}

	ul {list-style-type: disc;list-style-position: inside;margin: 0;
		li {font-weight: bold;}
	}

	ol {list-style-type: decimal;list-style-position: inside;margin: 0;
		li {font-weight: bold;}
	}

	& > p + p,
	& > ul + ul {margin: 10px 0 0 0;}

}

.alert-success {background-color: #dff0d8;color: #1e892a;
	a {color: #1e892a;}
}

.alert-info {background-color: #cfedfc;color: #267abe;
	a {color: #267abe;}
}

.alert-warning {background-color: #fff5c7;color: #c87100;
	a {color: #c87100;}
}

.alert-danger,
.alert-error {background-color: #ffcccc;color: #d10300;}

.alert-danger a,
.alert-error a {color: #d10300;}


.alert-form {border-color: #d10300 !important;}
.alert-form-info {display: none;margin: 5px 0 0 0;font-weight: bold;color: #d10300;}


.notify_success {color: #1e892a;}
.notify_info {color: #267abe;}
.notify_warning {color: #c87100;}
.notify_danger {color: #d10300;}




// 5.3 BUTTONS /////////////////////////////////////////////////////////////////////////////////////////////////////
.btn,
.btn a,
.btn-info,
.btn-primary {padding: 7px 10px;background: none !important;border: none !important;font-size: 14px;color: $color_1 !important;text-transform: uppercase;position: relative;
@include border_radius(0);
@include box_shadow(none !important);
@include transition( .3s, background, color );
	&:before {content: '';border: 1px solid $color_1;position: absolute;top: 0;right: 0;bottom: 0;left: 0;
	@include transition(.3s, right);
	}
}

.btn a {padding: 0 !important;border: none !important;}

.btn:hover,
.btn:hover a,
.btn-info:hover,
