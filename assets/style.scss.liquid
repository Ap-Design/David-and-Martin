	.product_wrapper {background: $bg_color;}

	.product_img {
		a {display: block;position: relative;
			&:after {content: '';border: 1px solid transparent;position: absolute;top: 0;right: 0;bottom: 0;left: 0;z-index: 100;
			@include transition(.3s, border-color);
              //height:210px;
			}
        
		}
      .img_change {
         		vertical-align: middle;
    			text-align: center;
        		padding-top: 20px;
			}
		img {background-color: $bg_color;position: relative;z-index: 1;
		-webkit-user-select: none;
				user-select: none;
		}
		.img__2 {position: absolute;left: 0;top: 0;z-index: 10;opacity: 0;}
	}

	.product_info {display: none;}

	.quick_view_btn {width: 50px;height: 50px;display: block;margin: 30px auto 0 auto;position: relative;
		&:before {content: '';width: 100%;height: 1px;background: rgba($color_1, .25);position: absolute;left: 0;top: 50%;
		@include transition(.3s, background);
		}
		&:after {content: '';width: 1px;height: 100%;background: rgba($color_1, .25);position: absolute;left: 50%;top: 0;
		@include transition(.3s, background);
		}
	}

	.product:hover {
		.product_img {
			a {
				&:after {border-color: $color_1;}
			}
		}

		.quick_view_btn {
			&:before,
			&:after {background: $color_1;}
		}
	}
}




