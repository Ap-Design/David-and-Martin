////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// COLOR SCHEMES SWITCH ////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
#color_settings {width: 240px;background: #fff;position: fixed;left: -240px;top: 200px;z-index: 100;
@include border_radius(0 4px 4px 0);
@include box_shadow(none);
@include transition(.3s, left, box-shadow);

	h4 {height: 50px;margin: 0;padding: 0 50px 0 25px;font: bold 14px/51px 'Arial';color: #333;}

	#color_settings__btn {width: 50px;height: 50px;overflow: hidden;background: #888;position: absolute;right: -50px;top: 0;
	@include transition(.3s, right);
	@include border_radius(0 4px 4px 0);
	@include box_shadow(0 0 10px 0 rgba(0, 0, 0, .25));

		i {width: 50px;height: 50px;display: block;font-size: 18px;line-height: 49px;color: #fff;text-align: center;
		text-shadow: 1px 1px 1px rgba(0, 0, 0, .7);
		}
	}

	&.on {left: 0;
	@include box_shadow(0 0 25px 5px rgba(0, 0, 0, .25));

		#color_settings__btn {right: 0;
		@include border_radius(4px);
		@include box_shadow(none);
		}
	}

	.color_palette {height: 30px;margin: 25px;overflow: hidden;border: 2px solid rgba(0, 0, 0, .2);position: relative;cursor: pointer;
	@include transition(.2s, background, box-shadow);
	@include border_radius(0);
	@include box_shadow( none );

		&.active {border-color: #fff;
		@include border_radius(2px);
		@include box_shadow( 0 0 0 2px #888 );
		}
	}

	.color_item {position: absolute;top: 0;bottom: 0;
		&.color_1 {left: -30px;right: 50%;z-index: 10;
		@include transform( skewX(-45deg) );
		}
		&.color_2 {left: 0;right: 0;z-index: 1;}
	}
}

#scheme_1 {
	.color_1 {background: {{ settings.scheme_1_color_1 }};}
	.color_2 {background: {{ settings.scheme_1_color_2 }};}
}

#scheme_2 {
	.color_1 {background: {{ settings.scheme_2_color_1 }};}
	.color_2 {background: {{ settings.scheme_2_color_2 }};}
}

#scheme_3 {
	.color_1 {background: {{ settings.scheme_3_color_1 }};}
	.color_2 {background: {{ settings.scheme_3_color_2 }};}
}

#scheme_4 {
	.color_1 {background: {{ settings.scheme_4_color_1 }};}
	.color_2 {background: {{ settings.scheme_4_color_2 }};}
}




{% endcapture %}




