		a {display: block;float: right;margin: 24px 0 0 10px;font-size: 14px;
			&.link_logout {position: absolute;right: 0;top: 0;}
		}
	}
}


// WELCOME MESSAGE
.account_welcome {text-align: center;
	.page_heading {
		&:before {content: '\f007';margin: 0 7px 0 0;font-family: 'FontAwesome';}
	}
}

.account_welcome__name {margin: 30px 0 0 0;color: $color_2;}

.account_welcome__desc {}


// ACCOUNT TABLES
.account_table {width: 100%;margin: 15px 0 0 0;
	th,
	td {vertical-align: middle;}
}


.table_info {
	tr {
		& ~ tr {border-top: 1px solid $border_color;}
	}
	td {padding: 5px 0;
		& ~ td {padding: 5px 0 5px 5px;color: $color_1;text-align: right;}
	}
}


.table_orders {
	thead {border-bottom: 2px solid $border_color;}
	th {padding: 5px 7px;border: 1px solid $border_color;color: $color_1;text-transform: uppercase;}

	tbody {}
	td {padding: 5px 7px;border: 1px solid $border_color;}

	tfoot {border-top: 2px solid $border_color;}

	p {margin: 7px 0 0 0;}

	a {text-decoration: underline;
		&:hover {text-decoration: none;}
	}

	.cell_fulfillment {}

	.cell_total {text-align: right;}

	.row_total {border-top: 2px solid $border_color;font-size: 14px;
		.money {font-weight: bold;}
	}
}


// DEFAULT ADDRESS
.account_default {
	.address_all {margin: 15px 0 0 0;
		a {text-decoration: underline;
			&:hover {text-decoration: none;}
		}
	}
}




// 13.4 ADDRESSES //////////////////////////////////////////////////////////////////////////////////////////////////
.account_address__item {
	h4 {padding: 0 0 7px 0;border-bottom: 1px solid $border_color;}
}




// 13.5 ORDERS /////////////////////////////////////////////////////////////////////////////////////////////////////
.order_cancelled {
	h4 {margin: 0;font-size: 14px;}
	p {margin: 10px 0 0 0;}
}




////////////////////////////////////////////////////////////////////////////////////////////////////////////////////
// 14. CART PAGE ///////////////////////////////////////////////////////////////////////////////////////////////////
////////////////////////////////////////////////////////////////////////////////////////////////////////////////////

#cart_loader {width: 100%;height: 0;position: fixed;left: 0;top: 0;
	&.loader_off .global_loader {top: 40px;}
	&.loader_on .global_loader {top: 80px;}
}


.cart_list {width: 100%;max-width: 100%;margin: 30px 0 0 0;
	th {padding: 15px 20px;background: rgba(0,0,0,.05);border: 1px solid $border_color;font-size: 14px;color: $color_1;text-transform: uppercase;
		&~ th {text-align: center;}
	}

	td {padding: 20px;border: 1px solid $border_color;vertical-align: middle;}

	.cell_3 {text-align: center;}
	.cell_5 {text-align: center;}
	.cart_price {font-size: 14px;text-align: right;}
}


.cart_item__img {width: 140px;float: left;}
.cart_item__info {margin: 0 0 0 160px;}
.cart_item__name {margin: -1% 0 0 0;}
.cart_item__variant {margin: 15px 0 0 0;font-size: 14px;color: $color_1;}

.cart_item__details {margin: 10px 0 0 0;
	p {margin: 5px 0 0 0;
		span {color: $color_1;}
	}
}


.cart_item {
	.quantity_box {width: 100px;height: 30px;margin: 0;position: relative;
		input {width: 42px;height: 30px;padding: 0;position: absolute;left: 29px;top: 0;}
		.quantity_down {width: 30px;height: 30px;margin: 0;position: absolute;left: 0;top: 0;}
		.quantity_up {width: 30px;height: 30px;margin: 0;position: absolute;right: 0;top: 0;}
	}
	.cart_update {width: 100px;margin: -1px 0 0 0;padding: 5px;font-size: 10px;}
}


.cart_item__remove {font-size: 14px;
	i {vertical-align: baseline;}
}


.cart_list .cart_buttons {
	.btn {float: right;margin: 0 0 0 20px;}
	.btn-alt {float: none;margin: 0;}
}


.cart_list .cart_summary {
	.cart_summary__row {margin: 0;padding: 0 0 10px 0;border-bottom: 1px solid $border_color;font-size: 16px;color: $color_1;
		& ~ .cart_summary__row {padding: 10px 0;}

		span {float: right;}
	}

	.cart_summary__notification {margin: 20px 0 0 0;}

	.cart_summary__instructions {margin: 15px 0 0 0;
		label {display: block;padding: 0 0 3px 0;font-weight: normal;color: $color_1;}
		textarea {width: auto !important;height: 100px !important;max-width: 100% !important;resize: none;}
	}

	.cart_summary__checkout {
		button {float: right;margin: 20px 0 0 10px;padding: 10px 20px;font-size: 16px;}
	}

	.cart_summary__methods {float: left;
		li {float: left;margin: 20px 10px 0 0;}
	}
}


// EMPTY CART NOTIFICATION
.cart_empty {
	.alert {}
	h4 {
		a {font-weight: inherit;text-decoration: underline;
			&:hover {text-decoration: none;}
		}
	}
}




